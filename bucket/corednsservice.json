{
    "version": "v0.1.0",
    "description": "coredns service wrapper",
    "homepage": "https://github.com/hnakamur/corednsservice",
    "bin": "corednsservice.exe",
    "pre_install": [
        "$src = \"\"",
        "$ByteArray = [System.Convert]::FromBase64String($src)",
        "if(!(test-path \"$dir\\corednsservice.yml\")) { [System.IO.File]::WriteAllBytes(\"$dir\\corednsservice.yaml\", $ByteArray) }",
        "if(!(test-path \"$dir\\Log\")) { New-Item \"$dir\\Log\" -ItemType Directory | Out-Null }",
        "if(!(test-path \"$dir\\Log\")) { New-Item \"$dir\\corefile\" | Out-Null }"
    ],
    "persist": [
        "corednsservice.yml",
        "log",
        "corefile"
    ],
    "url": "https://github.com/hnakamur/corednsservice/releases/download/v0.1.0/corednsservice.exe",
    "hash": "a1ecb70fb9fb8c0b4a56b7b971b64c78ff903a8cac2c936b9141651a5f2b4ae3"
}
