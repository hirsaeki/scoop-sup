{
    "version": "3.2.0",
    "description": "Japanese skk like IME.",
    "homepage": "https://github.com/nathancorvussolis/corvusskk",
    "checkver": "github",
    "license": "MS-PL",
    "url": "https://github.com/nathancorvussolis/corvusskk/releases/download/3.2.0/corvusskk-3.2.0.exe#/setup.exe",
    "hash": "e7cb3957a72e7e558061550bf2a5ae6ca7d1760d37fd804dc36a937fbfe07bc2",
    "installer": {
        "script": [
            "Start-Process -Wait \"$dir\\setup.exe\" -ArgumentList @('/quiet', '/norestart') -Verb RunAs",
            "Write-Host \"Need to restart Windows to complete install.\" -ForegroundColor Red"
        ],
        "keep": "true"
    },
    "post_install": [
        "if (!(Test-Path \"$env:AppData\\CorvusSKK\")){",
            "New-Item -ItemType Junction -Path \"$env:AppData\\CorvusSKK\" -Value \"$persist_dir\\corvusskk-appdata\"",
        "} elseif ([System.IO.Directory]::GetFileSystemEntries(\"$env:AppData\\CorvusSKK\")[0] -ne \"$persist_dir\\corvusskk-appdata\"){",
            "Start-Process -FilePath \"taskkill.exe\" -ArgumentList \"/im imcrvmgr.exe\" -NoNewWindow -Wait -PassThru | Out-Null",
            "Get-ChildItem \"$env:AppData\\CorvusSKK\" -Recurse | ForEach-Object {",
                "Move-Item $_.FullName (Join-Path \"$persist_dir\\corvusskk-appdata\" $_.FullName.Substring($Item.Length + 1))",
            "}",
            "Remove-Item \"$env:AppData\\CorvusSKK\" -Force",
            "New-Item -ItemType Junction -Path \"$env:AppData\\CorvusSKK\" -Value \"$persist_dir\\corvusskk-appdata\"",
            "Start-Process -FilePath \"$env:SystemRoot\\System32\\IME\\IMCRVSKK\\imcrvmgr.exe\"",
        "}",
        "if (!(Test-Path $persist_dir\\corvusskk-appdata\\config.xml)){",
            "\"Create Config\"",
            "$src = \"\"",
            "$ByteArray = [System.Convert]::FromBase64String($src)",
            "[System.IO.File]::WriteAllBytes(\"$dir\\config.zip\", $ByteArray)",
            "Expand-Archive -Path $dir\\config.zip -DestinationPath $dir\\corvusskk-appdata",
            "Remove-Item $dir\\config.zip",
        "}"
    ],
    "persist": [
        "corvusskk-appdata"
    ],
    "uninstaller": {
        "script": [
            "Start-Process -Wait \"$dir\\setup.exe\" -ArgumentList @('/uninstall', '/quiet', '/norestart') -Verb RunAs",
            "Write-Host \"Need to restart Windows to complete uninstall.\" -ForegroundColor Red"
        ]
    },
    "autoupdate": {
        "url": "https://github.com/nathancorvussolis/corvusskk/releases/download/$version/corvusskk-$version.exe#/setup.exe"
    }
}
