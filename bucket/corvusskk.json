{
    "version": "3.3.0",
    "description": "Japanese skk like IME.",
    "homepage": "https://github.com/nathancorvussolis/corvusskk",
    "checkver": "github",
    "license": "MS-PL",
    "url": "https://github.com/nathancorvussolis/corvusskk/releases/download/3.3.0/corvusskk-3.3.0.exe#/setup.exe",
    "hash": "dce4682d88e4686856ea4680c32d195aaf73fa43b4d92308ff7b48fc74a770a0",
    "installer": {
        "script": [
            "Start-Process -Wait \"$dir\\setup.exe\" -ArgumentList @('/quiet', '/norestart') -Verb RunAs",
            "Write-Host \"Need to restart Windows to complete install.\" -ForegroundColor Red"
        ],
        "keep": true
    },
    "post_install": [
        "Stop-Process -Name imcrvmgr -Force -ErrorAction SilentlyContinue",
        "$appdata = Get-Item \"$env:APPDATA\\CorvusSKK\" -ErrorAction SilentlyContinue",
        "if (!(Test-Path \"$env:APPDATA\\CorvusSKK\\config.xml\")){",
        "  \"Create Config\"",
        "  $src = \"\"",
        "  $ByteArray = [System.Convert]::FromBase64String($src)",
        "  [System.IO.File]::WriteAllBytes(\"$dir\\config.zip\", $ByteArray)",
        "  Expand-Archive -Path $dir\\config.zip -DestinationPath \"$env:APPDATA\\CorvusSKK\"",
        "  $acl = Get-Acl \"$env:APPDATA\\CorvusSKK\\config.xml\"",
        "  $accessRule = New-Object System.Security.AccessControl.FileSystemAccessRule(\"ALL APPLICATION PACKAGES\",\"Read\",\"Allow\")",
        "  $acl.SetAccessRule($accessRule)",
        "  Set-Acl \"$env:APPDATA\\CorvusSKK\\config.xml\" $acl",
        "  Remove-Item $dir\\config.zip",
        "}",
        "Start-Process -FilePath \"$env:SystemRoot\\System32\\IME\\IMCRVSKK\\imcrvmgr.exe\" -ErrorAction SilentlyContinue"
    ],
    "persist": "corvusskk-appdata",
    "uninstaller": {
        "script": [
            "Start-Process -Wait \"$dir\\setup.exe\" -ArgumentList @('/uninstall', '/quiet', '/norestart') -Verb RunAs",
            "Write-Host \"Need to restart Windows to complete uninstall.\" -ForegroundColor Red"
        ]
    },
    "autoupdate": {
        "url": "https://github.com/nathancorvussolis/corvusskk/releases/download/$version/corvusskk-$version.exe#/setup.exe"
    }
}
